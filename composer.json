{
	"name": "evergreenphp/evergreen",
	"type": "project",
	"description": "EvergreenPHP Framework.",
	"keywords": [
		"framework",
		"evergreen",
		"evergreenphp"
	],
	"license": "MIT",
	"require": {
		"php": "^7.3|^8.0",
		"evergreenphp/evergreen": "0.1.*"
	},
	"require-dev": {
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-autoload-dump": [
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
		]
	}
}